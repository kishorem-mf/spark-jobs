CREATE TABLE public.channel_mapping
(
    COUNTRY_CODE VARCHAR(2),
    ORIGINAL_CHANNEL VARCHAR(256),
    LOCAL_CHANNEL VARCHAR(256),
    CHANNEL_USAGE VARCHAR(256),
    CHANNEL_REFERENCE_FK INT,
    CONSTRAINT channel_mapping_COUNTRY_CODE_ORIGINAL_CHANNEL_pk PRIMARY KEY (COUNTRY_CODE, ORIGINAL_CHANNEL),
    CONSTRAINT channel_mapping_channel_references_channel_reference_id_fk FOREIGN KEY (CHANNEL_REFERENCE_FK) REFERENCES channel_references (CHANNEL_REFERENCE_ID)
);

CREATE TABLE public.channel_references
(
    CHANNEL_REFERENCE_ID INT,
    SOCIAL_COMMERCIAL VARCHAR(256),
    STRATEGIC_CHANNEL VARCHAR(256),
    GLOBAL_CHANNEL VARCHAR(256),
    GLOBAL_SUBCHANNEL VARCHAR(256),
    CREATED_AT TIMESTAMP,
    UPDATED_AT TIMESTAMP,
    CONSTRAINT channel_references_CHANNEL_REFERENCE_ID_pk PRIMARY KEY (CHANNEL_REFERENCE_ID)
);

SELECT *
FROM public.channel_mapping
INNER JOIN channel_references reference ON channel_mapping.channel_reference_fk = reference.channel_reference_id
;
